{
  "name": "template-vue-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:mock": "USE_MOCK=true vite",
    "dev:sandbox": "vite --mode sandbox",
    "build": "vue-tsc --noEmit && npm run build:types && vite build",
    "build:sandbox": "vue-tsc --noEmit && npm run build:types && vite build --mode sandbox",
    "build:report": "BUILD_VISUALIZER=1 npm run build",
    "build:sourcemap": "BUILD_SOURCEMAP=1 npm run build",
    "preview": "vite preview",
    "lint": "eslint --cache --ext .vue,.ts,.js src/",
    "lint:fix": "eslint --cache --ext .vue,.ts,.js src/ --fix",
    "tsc": "vue-tsc --noEmit",
    "ws": "node server/index.mjs",
    "prepare": "husky install && npm run build:types",
    "test": "NODE_ENV=test vitest test/integration.test",
    "test:sandbox": "NODE_ENV=test vitest --mode sandbox.test test/integration.test",
    "coverage": "vitest run --coverage",
    "build:types": "zx ./scripts/build-types.mjs",
    "postinstall": "zx ./scripts/copy-shiki-files.mjs"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@codemirror/autocomplete": "^6.3.0",
    "@codemirror/commands": "^6.0.1",
    "@codemirror/lang-javascript": "^6.0.1",
    "@codemirror/lang-json": "^6.0.0",
    "@codemirror/language": "^6.2.1",
    "@codemirror/search": "^6.2.1",
    "@codemirror/state": "^6.1.0",
    "@codemirror/legacy-modes": "^6.3.3",
    "@codemirror/theme-one-dark": "^6.0.0",
    "@codemirror/view": "^6.0.3",
    "@fontsource-variable/inter": "^5.0.5",
    "@sentry/tracing": "^7.30.0",
    "@sentry/vue": "^7.30.0",
    "@types/debug": "^4.1.7",
    "@types/lodash-es": "^4.17.6",
    "@types/nprogress": "^0.2.0",
    "@typescript/vfs": "^1.4.0",
    "@vueuse/core": "^9.2.0",
    "ansi_up": "^5.1.0",
    "ant-design-vue": "^3.2.9",
    "axios": "^0.27.2",
    "bson": "^4.6.5",
    "chart.js": "^4.2.1",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.3",
    "debug": "^4.3.3",
    "diff": "^5.1.0",
    "diff2html": "^3.4.22",
    "eventemitter3": "^4.0.7",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "mixpanel-browser": "^2.47.0",
    "nanoid": "^4.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "rehype-external-links": "^2.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "shepherd.js": "^11.0.0",
    "shiki": "^0.14.3",
    "socket.io-client": "^4.5.2",
    "ua-parser-js": "^1.0.2",
    "unified": "^10.1.2",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.16",
    "vue-virtual-scroller": "2.0.0-beta.3"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^0.4.0",
