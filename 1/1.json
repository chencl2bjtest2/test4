    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "fast-glob": "^3.2.12",
    "happy-dom": "^7.0.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
